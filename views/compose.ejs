<%- include('partials/header') %> 

<h1>Compose</h1>

<form action="/compose" method="post">
  <div class="mb-3">
    <label for="title" class="form-label">Title</label>
    <input type="text" class="form-control" id="title" name="postTitle" >
  </div>
  <div class="mb-3" >
    <label for="post" class="form-label">Post</label>
    <textarea type="text" class="form-control" id="post" name="postContent"  rows="6"></textarea>
  </div>
  
  <button type="submit" class="btn btn-primary"><i class="bi bi-check2-circle"></i>Publish</button>
</form>


<%- include('partials/footer') %>

